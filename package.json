{
  "name": "trsn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "git config core.hooksPath .githooks",
    "lint": "eslint",
    "format": "prettier --write .",
    "test:vitest": "mkdir -p reports/vitest && vitest run --coverage && node tests/vitest/report.cjs",
    "test:jest": "mkdir -p reports/jest && jest",
    "test:playwright": "playwright test",
    "test:playwright:report": "npx playwright show-report reports/playwright",
    "test:playwright:screenshot": "npx playwright screenshot --full-page file://$PWD/reports/playwright/index.html public/report-screenshots/playwright.png",
    "test:k6": "mkdir -p reports/k6 && K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=reports/k6/web-dashboard.html k6 run --summary-export=reports/k6/summary.json tests/k6/smoke.js && node tests/k6/report.cjs"
  },
  "dependencies": {
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jest": "^29.5.14",
    "@vitest/coverage-v8": "^2.1.7",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.2.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.5",
    "typescript": "^5",
    "vitest": "^2.1.7"
  }
}
